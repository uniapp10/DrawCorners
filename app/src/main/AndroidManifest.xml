<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhudongdong.drawcorners">

    <uses-permission
        android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zdd.td.mine.Activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zdd.td.mine.Activity.DrawingActivity"
            android:label="@string/title_activity_drawing" />
        <activity android:name="com.zdd.td.mine.Activity.MainActivity"></activity>

        <activity android:name="com.zdd.td.mine.Activity.AccountDetailActivity"/>
        <activity
            android:name="com.zdd.td.mine.Fragments.MineActivity.MineProductDetailActivity"/>
    </application>

</manifest>